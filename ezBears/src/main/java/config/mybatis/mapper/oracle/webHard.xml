<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ez.ezBears.webhard.model.WebHardDAO">

	<select id="selectAllWebHard" parameterType="int" resultType="webHardVo">
		select *
		from web_hard
		where my_board_no = #{myboardNo}
	</select>


</mapper>